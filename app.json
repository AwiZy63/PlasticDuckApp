{
  "expo": {
    "name": "plasticduck",
    "slug": "plasticduck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/plasticduck/icon.png",
    "splash": {
      "image": "./assets/plasticduck/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#161616"
      },
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "fr.plasticduck.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
